<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="XTextView">
        <attr name="fontFamily" format="enum">
            <enum name="lato" value="1" />
            <enum name="bebas" value="2" />
            <enum name="us" value="3"/>
            <enum name="champagne" value="4"/>
        </attr>

        <attr name="fontStyle" format="enum">
            <enum name="regular" value="0" />
            <enum name="bold" value="1" />
        </attr>
    </declare-styleable>
</resources>